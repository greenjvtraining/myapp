<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Users Page</h1>
    <hr>
    <table border="1">
        <thead>
            <tr>
                <th>name</th><th>country</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
            <tr>
                <td><a href="/user?name=<%= user.name %>"><%= user.name %></a></td>

                <td><%= user.country %></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>